            "countryName": "India",
            "currency": "INR",
            "selector": "div.p-image > a",
            "actionType": "GET_ALL_LINKS",
            "pageActions": [
                {
                    "key": "ImageURL",
                    "locate": {
                        "by": "CSS_SELECTOR",
                        "byValue": "div#wrap > a.cloud-zoom > img"
                    },
                    "actionType": "GET_LINK"
                },
                {
                    "key": "Product_Name",
                    "locate": {
                        "by": "CSS_SELECTOR",
                        "byValue": "form#buy_block > h1.title-5"
                    },
                    "actionType": "GET_TEXT"
                },
                {
                    "key": "Price",
                    "locate": {
                        "by": "CSS_SELECTOR",
                        "byValue": "span.final-pd-price"
                    },
                    "actionType": "GET_TEXT"
                },
                {
                    "key": "Description",
                    "locate": {
                        "by": "CSS_SELECTOR",
                        "byValue": "div.desc > span#short_desc"
                    },
                    "actionType": "GET_TEXT"
                },
                {
                    "key": "Raw_Category",
                    "locate": {
                        "by": "CSS_SELECTOR",
                        "byValue": "div#breadcrumb > ul > li:nth-child(3) > a"
                    },
                    "actionType": "GET_TEXT"
                },
                {
                    "key": "Weight",
                    "locate": {
                        "by": "XPATH",
                        "byValue": "//*[@id=\"product_total_weight\"]"
                    },
                    "transform": {
                        "type": "lambda",
                        "value": "lambda x: x.split(' ')[0]"
                    },
                    "actionType": "GET_TEXT",
                    "unit": "gram"
                },
                {
                    "key": "Gold_Colour",
                    "locate": {
                        "by": "CSS_SELECTOR",
                        "byValue": "div.selected > span.color-val"
                    },
                    "actionType": "GET_TEXT"
                },
                {
                    "key": "Gold_Purity",
                    "locate": {
                        "by": "CSS_SELECTOR",
                        "byValue": "label#metalPurityId"
                    },
                    "actionType": "GET_TEXT",
                    "unit": "karat"
                },
                {
                    "key": "Diamond_Clarity",
                    "locate": {
                        "by": "XPATH",
                        "byValue": "//*[@id=\"selectMetalAndDiamondQuality\"]/div[2]/div/div[2]/label"
                    },
                    "transform": {
                        "type": "lambda",
                        "value": "lambda x: x.split(' ')[0]"
                    },
                    "actionType": "GET_TEXT"
                },
                {
                    "key": "Diamond_Colour",
                    "locate": {
                        "by": "XPATH",
                        "byValue": "//*[@id=\"selectMetalAndDiamondQuality\"]/div[2]/div/div[2]/label"
                    },
                    "transform": {
                        "type": "lambda",
                        "value": "lambda x: x.split(' ')[-1]"
                    },
                    "actionType": "GET_TEXT"
                },
                {
                    "key": "Diamond_Weight",
                    "locate": {
                        "by": "CSS_SELECTOR",
                        "byValue": "div.weight-tooltip + span"
                    },
                    "transform": {
                        "type": "lambda",
                        "value": "lambda x: x.split(' ')[0]"
                    },
                    "actionType": "GET_TEXT",
                    "unit": "carat"
                },
                {
                    "key": "Diamond_Pieces",
                    "locate": {
                        "by": "CSS_SELECTOR",
                        "byValue": "div.t-d-d.last > span"
